{
  "name": "react-template",
  "version": "0.1.0",
  "dependencies": {
    "@babel/standalone": "^7.24.5",
    "@monaco-editor/react": "^4.6.0",
    "@typescript/ata": "^0.9.4",
    "ahooks": "^3.7.8",
    "github-markdown-css": "^5.4.0",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "luke-awesome-query": "^0.1.12",
    "markdown-it-github": "^0.5.0",
    "react-router": "^6.17.0",
    "react-router-dom": "^6.17.0",
    "zustand": "^4.5.2"
  },
  "scripts": {
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d build",
    "start:dev": "node ./scripts/build.js --env=dev && react-scripts start",
    "start:prod": "node ./scripts/build.js --env=prod && react-scripts start",
    "start:test": "node ./scripts/build.js --env=test && react-scripts start",
    "build:dev": "node ./scripts/build.js --env=dev && react-scripts build",
    "build:test": "node ./scripts/build.js --env=test && react-scripts build",
    "build:prod": "node ./scripts/build.js --env=prod && react-scripts build",
    "build": "node ./scripts/build.js --env=prod && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "precommit": "npm run predeploy && npm run deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.8.2",
    "@tanstack/react-query-devtools": "^5.8.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__standalone": "^7.1.7",
    "@types/jest": "^27.5.2",
    "@types/markdown-it": "^13.0.6",
    "@types/node": "^16.18.24",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "gh-pages": "^6.0.0",
    "markdown-it": "^13.0.2",
    "minimist": "^1.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.69.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yaml": "^2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz.config.js"
    }
  },
  "homepage": "https://hellolukeding.github.io",
  "volta": {
    "node": "18.20.3"
  }
}
